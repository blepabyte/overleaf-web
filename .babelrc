{
  "presets": [
    "@babel/react",
    ["@babel/env", {
      "useBuiltIns": "usage",
      "corejs": { "version": 3 }
    }]
  ],
  "plugins": ["angularjs-annotate"],
  // Target our current Node version in test environment, to transform and
  // polyfill only what's necessary
  "env": {
    "test": {
      "presets": [
        "@babel/react",
        ["@babel/env", {
            "targets": {"node": "10.21"},
            "useBuiltIns": "usage",
            "corejs": { "version": 3 }
          }]
      ]
    }
  }
}
